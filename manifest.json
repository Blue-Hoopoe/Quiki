{
    "manifest_version": 2,
    "name": "quiki",
    "short_name": "quiki",
    "homepage_url": "https://github.com/Blue-Hoopoe/quiki",
    "description": "Nieoficjalny dodatek korzystający z angielsko-polskiego słownika diki.pl. Uwaga: jest to wersja alpha.",
    "version": "0.1.28",
    "version_name": "0.1.28 (alpha)",
    "author": [{
        "name": "Maciej Dudek",
        "a.k.a.": "Blue-Hoopoe",
        "email": "quiki.extension@gmail.com"
    }],
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "64": "icons/icon_64.png",
        "128": "icons/icon_128.png",
        "256": "icons/icon_256.png",
        "512": "icons/icon_512.png"
    },
    "browser_action": {
        "default_icon": "icons/icon_512.png",
        "default_popup": "pages/popup.html"
    },
    "background": {
        "scripts": [
            "modules/QuikiStorageManager.js",
            "modules/QuikiService.js",
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": [
            "vendor/jquery-3.3.1.min.js",
            "modules/QuikiClient.js",
            "content-script.js"
        ],
        "css": [
            "styles/quiki-modal.css"
        ],
        "run_at": "document_start"
    }],
    "permissions": [],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Q"
            },
            "description": "Toggle feature foo"
        }
    }
}